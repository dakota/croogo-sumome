{
    "name": "dakota/sumome",
    "description": "Free Tools to grow your email list from SumoMe.com for Croogo",
    "type": "cakephp-plugin",
    "require": {
        "croogo/croogo": "^4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "SumoMe\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SumoMe\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
